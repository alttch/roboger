#!/usr/bin/env python3

__version__ = '2.0.41'

product_build = 'stable'

from pathlib import Path
from pyaltt2.app import manage_gunicorn_app

dir_me = Path(__file__).absolute().parents[1]

manage_gunicorn_app('roboger',
                    dir_me,
                    version=__version__,
                    build=product_build,
                    default_port=7719,
                    api_uri='/')
